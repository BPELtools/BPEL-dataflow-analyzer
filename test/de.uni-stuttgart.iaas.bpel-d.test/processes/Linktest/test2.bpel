<!-- test2 BPEL Process [Generated by the Eclipse BPEL Designer] -->
<bpel:process name="test2"
         targetNamespace="http://www.gyy.com/test2NS"
         suppressJoinFailure="yes"
         xmlns:tns="http://www.gyy.com/test2NS"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="test2Artifacts.wsdl" namespace="http://www.gyy.com/test2NS" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <bpel:partnerLink myRole="test2Provider"
            name="customer" partnerLinkType="tns:test2" partnerRole="test2Provider"/>
        <bpel:partnerLink myRole="test2Provider"
            name="orderProcessor"
            partnerLinkType="tns:test2" partnerRole="test2Provider"/>
        <bpel:partnerLink myRole="test2Provider"
            name="pymtProcessor" partnerLinkType="tns:test2" partnerRole="test2Provider"/>
    </bpel:partnerLinks>
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <bpel:variable messageType="tns:orderInfoMessage" name="orderInfo"/>
        <bpel:variable messageType="tns:pymtInfoMessage" name="pymtInfo"/>
        <bpel:variable messageType="tns:responseMessage" name="response"/>


    </bpel:variables>
    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
 
    <bpel:flow name="Flow"><bpel:links>
            <bpel:link name="link1" />
            <bpel:link name="link2" />
            <bpel:link name="link6" />
            <bpel:link name="link7" />
        </bpel:links>
        <!-- Receive input from requester. 
             Note: This maps to operation defined in test2.wsdl 
             -->
             <bpel:receive createInstance="yes" name="A" partnerLink="client" portType="tns:test2" operation="process" variable="input"> 
            <bpel:sources>
                <bpel:source linkName="link1" />
                <bpel:source linkName="link2" />
            </bpel:sources>
        </bpel:receive>

        <!-- Generate reply to synchronous request -->
        <bpel:assign name="C" validate="no">
                <bpel:targets>
                	<bpel:target linkName="link2" />
            	</bpel:targets>
            	<bpel:sources>
            		<bpel:source linkName="link6" />
            	</bpel:sources>          
				<bpel:copy>
                	<bpel:from>
                    	<bpel:literal>ABC</bpel:literal>
                	</bpel:from>
                	<bpel:to part="text" variable="response" />
            	</bpel:copy>
     </bpel:assign>
     
            <bpel:reply name="G" partnerLink="client" portType="tns:test2" operation="process" variable="output">
            	<bpel:targets>  
            		<bpel:target linkName="link7" /> 
            	</bpel:targets>            
            </bpel:reply>
        

    
    
    
        
        <bpel:if name="If" xmlns:http="urn:http:namesapce">
            <bpel:targets>
                	<bpel:target linkName="link1" />
            	</bpel:targets><bpel:assign validate="no" name="D">
            		<bpel:copy>
                		<bpel:from>
                    		<bpel:literal>Dear customer, ...</bpel:literal>
                		</bpel:from>
                	<bpel:to part="text" variable="response" />
            		</bpel:copy>            
            	</bpel:assign>
            <bpel:elseif>
                <bpel:assign validate="no" name="E">  
            	<bpel:copy>
                	<bpel:from>
                    	<bpel:literal>Liebe, Kunden...</bpel:literal>
                	</bpel:from>
                	<bpel:to part="text" variable="response" />
            	</bpel:copy>            
            
                </bpel:assign>
</bpel:elseif>
            <bpel:else>
                <bpel:assign validate="no" name="F">
            	<bpel:targets>  
            		<bpel:target linkName="link6" /> 
            	</bpel:targets>
            	<bpel:sources>  
            		<bpel:source linkName="link7">          
            			<bpel:transitionCondition><![CDATA[$input.payload/tns:input='other']]></bpel:transitionCondition>
            		</bpel:source>
            	</bpel:sources>            
            		<bpel:copy>
                		<bpel:from>
                    		<bpel:literal>false, ...</bpel:literal>
                		</bpel:from>
                	<bpel:to part="text" variable="response" />
            		</bpel:copy>            
            	</bpel:assign>
            </bpel:else>

        
        
        
        </bpel:if>
    
    </bpel:flow>
</bpel:process>

