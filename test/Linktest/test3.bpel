<!-- test3 BPEL Process [Generated by the Eclipse BPEL Designer] -->
<bpel:process name="test3"
         targetNamespace="http://test3NS.com"
         suppressJoinFailure="yes"
         xmlns:tns="http://test3NS.com"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="test3Artifacts.wsdl" namespace="http://test3NS.com" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:test3"
                     myRole="test3Provider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:test3RequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:test3ResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->      

        <bpel:flow name="Flow">
        <bpel:links>
            
            <bpel:link name="link2" />
            <bpel:link name="link5" />
            <bpel:link name="link6" />
            <bpel:link name="link1" />
        </bpel:links>   


<!-- Receive input from requester. 
             Note: This maps to operation defined in PickProcess.wsdl 
             -->
        
        
        <!-- Generate reply to synchronous request -->
        <bpel:receive createInstance="yes" name="A" partnerLink="client" portType="tns:test2" operation="process" variable="input"> 
            <bpel:sources>
                <bpel:source linkName="link1" />
                <bpel:source linkName="link2" />
            </bpel:sources>         
        </bpel:receive>     
           
        <bpel:assign name="C" validate="no">
                <bpel:targets>
                	<bpel:target linkName="link2" />
            	</bpel:targets>
            	<bpel:sources>
            		<bpel:source linkName="link5" />
            	</bpel:sources>          
				<bpel:copy>
                	<bpel:from>
                    	<bpel:literal>ABC</bpel:literal>
                	</bpel:from>
                	<bpel:to part="test" variable="output" />
            	</bpel:copy>
     </bpel:assign>        
    

        <bpel:pick name="B" createInstance="yes"><bpel:onMessage partnerLink="client" operation="process" portType="tns:PickProcess" variable="input">
                <bpel:targets>
                	<bpel:target linkName="link1" />
            	</bpel:targets>
  
            <bpel:assign validate="no" name="D">
           
            <bpel:copy>
               	<bpel:from>
                 	<bpel:literal xml:space="preserve"><tns:PickProcessResponse xmlns:tns="http://gyy.com/PickProcess" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  					<tns:result>process</tns:result>
				</tns:PickProcessResponse></bpel:literal>
                </bpel:from>
                <bpel:to part="test" variable="output"></bpel:to>
            </bpel:copy>
            </bpel:assign>
                    
                
            </bpel:onMessage>
            
            <bpel:onMessage partnerLink="client" operation="work" portType="tns:PickProcess" variable="input">
 
                
                    
                   
                    <bpel:assign validate="no" name="E"><bpel:targets>
                			<bpel:target linkName="link5" />
            			</bpel:targets>
            			<bpel:sources>
            				<bpel:source linkName="link6" />
            			</bpel:sources>     
                        <bpel:copy>
                            <bpel:from>
                                <bpel:literal xml:space="preserve"><tns:workResponse xmlns:tns="http://gyy.com/PickProcess" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  								<tns:out>work</tns:out>
								</tns:workResponse>
								</bpel:literal>
                            </bpel:from>
                            <bpel:to variable="output" part="test"></bpel:to>
                        </bpel:copy>
                        
                    
                    </bpel:assign>
              
            </bpel:onMessage>
        	<bpel:onAlarm for="'PT15M'">
    
     			
                    
                <bpel:throw faultName="throw" faultVariable="output"></bpel:throw>
            </bpel:onAlarm>
        	
        
        
            <bpel:targets>
                <bpel:target linkName="link1"></bpel:target>
            </bpel:targets>
        </bpel:pick>
        <bpel:reply name="F" partnerLink="client" portType="tns:PickProcess" operation="process" variable="output" >
         <bpel:targets>
            <bpel:target linkName="link6" />
        </bpel:targets> 
        </bpel:reply>
        </bpel:flow>
</bpel:process>

