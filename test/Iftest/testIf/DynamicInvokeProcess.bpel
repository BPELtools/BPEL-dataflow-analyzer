<!-- DynamicInvokeProcess BPEL Process [Generated by the Eclipse BPEL Designer] -->
<bpel:process name="DynamicInvokeProcess"
         targetNamespace="http://gyy.com/DynamicInvokeProcess"
         suppressJoinFailure="yes"
         xmlns:tns="http://gyy.com/DynamicInvokeProcess"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns="http://www.se.uni-hannover.de/soa08/tutorial/HelloWorldService"
         xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref"
         >

    <!-- Import the client WSDL -->
    <bpel:import namespace="http://gyy.com/HelloWorldService" location="HelloWorldService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import location="DynamicInvokeProcessArtifacts.wsdl" namespace="http://gyy.com/DynamicInvokeProcess"
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:DynamicInvokeProcess"
                     myRole="DynamicInvokeProcessProvider"
                     />
        <bpel:partnerLink name="HelloWorldServicePartnerLink" partnerLinkType="tns:HelloWorldPartnerLinkType" partnerRole="HelloWorldProvider"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:DynamicInvokeProcessRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                messageType="tns:DynamicInvokeProcessResponseMessage"/>
        
        
        <bpel:variable name="HelloWorldServicePartnerLinkResponse"
        		messageType="ns:getMessageResponse"></bpel:variable>
        <bpel:variable name="HelloWorldServicePartnerLinkRequest"
        		messageType="ns:getMessageRequest"></bpel:variable>
        <bpel:variable name="HelloWorldEndpoint"
        		element="sref:service-ref"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in DynamicInvokeProcess.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:DynamicInvokeProcess"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:if name="ChooseLanguage" xmlns:http="urn:http:namesapce">
            <bpel:condition><![CDATA[$input.payload/tns:input='ger']]></bpel:condition>
            <bpel:assign validate="no" name="German">
                <bpel:copy>
                    <bpel:from>
                        <bpel:literal xml:space="preserve"><sref:service-ref xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref">
<EndpointReference xmlns="http://www.w3.org/2005/08/addressing">
	<Address>http://hobbes.se.uni-hannover.de:8080/HelloWorldServices/services/GermanHelloWorldService</Address>
</EndpointReference>
</sref:service-ref></bpel:literal>
                    </bpel:from>
                    <bpel:to variable="HelloWorldEndpoint"></bpel:to>
                </bpel:copy>
            </bpel:assign>
            <bpel:elseif>
                <bpel:condition><![CDATA[$input.payload/tns:input='esp']]></bpel:condition>
                <bpel:assign validate="no" name="Spanish">
                    <bpel:copy>
                        <bpel:from>
                            <bpel:literal xml:space="preserve"><sref:service-ref xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref">
<EndpointReference xmlns="http://www.w3.org/2005/08/addressing">
	<Address>http://hobbes.se.uni-hannover.de:8080/HelloWorldServices/services/SpanishHelloWorldService</Address>
</EndpointReference>
</sref:service-ref></bpel:literal>
                        </bpel:from>
                        <bpel:to variable="HelloWorldEndpoint"></bpel:to>
                    </bpel:copy>
                </bpel:assign>
            </bpel:elseif><bpel:elseif>
                <bpel:condition><![CDATA[$input.payload/tns:input='fra']]></bpel:condition>
                <bpel:assign validate="no" name="French">
                    <bpel:copy>
                        <bpel:from>
                            <bpel:literal xml:space="preserve"><sref:service-ref xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref">
<EndpointReference xmlns="http://www.w3.org/2005/08/addressing">
	<Address>http://hobbes.se.uni-hannover.de:8080/HelloWorldServices/services/FrenchHelloWorldService</Address>
</EndpointReference>
</sref:service-ref></bpel:literal>
                        </bpel:from>
                        <bpel:to variable="HelloWorldEndpoint"></bpel:to>
                    </bpel:copy>
                </bpel:assign>
            </bpel:elseif><bpel:else>
                <bpel:assign validate="no" name="English">
                    <bpel:copy>
                        <bpel:from>
                            <bpel:literal xml:space="preserve"><sref:service-ref xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref">
<EndpointReference xmlns="http://www.w3.org/2005/08/addressing">
	<Address>http://hobbes.se.uni-hannover.de:8080/HelloWorldServices/services/EnglishHelloWorldService</Address>
</EndpointReference>
</sref:service-ref></bpel:literal>
                        </bpel:from>
                        <bpel:to variable="HelloWorldEndpoint"></bpel:to>
                    </bpel:copy>
                </bpel:assign>
            </bpel:else></bpel:if>
        <bpel:assign validate="no" name="AssignEndpoint">
            <bpel:copy>
                <bpel:from variable="HelloWorldEndpoint"></bpel:from>
                <bpel:to partnerLink="HelloWorldServicePartnerLink"></bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="DynamicHelloWorldInvoke" partnerLink="HelloWorldServicePartnerLink" operation="getMessage" portType="ns:HelloWorldServicePortType" outputVariable="HelloWorldServicePartnerLinkResponse"></bpel:invoke>
        <bpel:assign validate="no" name="AssignOutput">
            
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:DynamicInvokeProcessResponse xmlns:tns="http://www.se.uni-hannover.de/soa08/tutorial/DynamicInvokeProcess" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result></tns:result>
</tns:DynamicInvokeProcessResponse>
</bpel:literal>
                </bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="HelloWorldServicePartnerLinkResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[message]]>
                    </bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[tns:result]]>
                    </bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:DynamicInvokeProcess"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

